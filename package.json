{
  "name": "dispatch",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0 <11"
  },
  "os": [
    "darwin",
    "linux",
    "!win32"
  ],
  "description": "Dispatch Presentation Layer",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.prod.js",
    "start:watchman": "watchman -f watch-project .",
    "start": "node app.js",
    "start:dev": "NODE_ENV=development webpack-dev-server --config webpack.dev.js --port 3000 --host=0.0.0.0",
    "test": "NODE_ENV=mocha-testing nyc mocha --watch --timeout=3000 ./test/*/*test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DispatchResponse/dispatchResponseApp.git"
  },
  "author": "Kevin Coyner, Nick Freeman",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "https://dispatchresponse.com",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "6.26.3",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "compression-webpack-plugin": "1.1.11",
    "css-loader": "1.0.0",
    "date-fns": "^1.29.0",
    "dotenv": "5.0.1",
    "env2": "2.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.30.1",
    "luxon": "1.0.0",
    "nodemon": "1.18.3",
    "path": "0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "5.4.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "7.0.3",
    "serve-favicon": "2.4.5",
    "style-loader": "0.21.0",
    "styled-components": "^2.4.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "webpack": "3.12.0",
    "webpack-merge": "4.1.3",
    "webpack-node-externals": "1.7.2",
    "webpack-plugin": "1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "enzyme": "^3.6.0",
    "enzyme-to-json": "3.3.3",
    "eslint": "4.19.1",
    "jest": "22.4.2",
    "npm": "^5.6.0",
    "raf": "3.4.0",
    "react-test-renderer": "16.2.0",
    "webpack-cli": "2.0.10",
    "webpack-dev-server": "2.11.1"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".*.svg"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
